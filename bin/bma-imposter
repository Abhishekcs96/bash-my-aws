#!/usr/bin/env bash

# bma-imposter - replace aws with a wrapper around cachews
#
# Runs given command with BMA bin/ prepended to $PATH
#
# This means all bash-my-aws describe/list functions should work via cachews
#
# cachews gives you caching for web services

SLF_DIR="$(readlink -f "$(dirname -- "${BASH_SOURCE[0]:-${0:A}}")/../")"
export PATH="${SLF_DIR}/bin:${PATH}"

bma $@
